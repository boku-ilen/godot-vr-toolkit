; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Spatial",
"class": "ControllerTool",
"language": "GDScript",
"path": "res://addons/vr-toolkit/Controller/ControllerTool.gd"
}, {
"base": "RigidBody",
"class": "InteractableObject",
"language": "GDScript",
"path": "res://addons/vr-toolkit/Objects/InteractableObject.gd"
}, {
"base": "Control",
"class": "VRGuiElement",
"language": "GDScript",
"path": "res://addons/vr-toolkit/Gui/GuiElement.gd"
} ]
_global_script_class_icons={
"ControllerTool": "",
"InteractableObject": "",
"VRGuiElement": ""
}

[application]

config/name="VR-Toolkit"
run/main_scene="res://demo/test.tscn"
config/icon="res://icon.png"

[autoload]

GlobalVRAccess="*res://addons/vr-toolkit/GlobalVRAccess.gd"

[gdnative]

singletons=[ "res://addons/godot-openvr/godot_openvr.gdnlib" ]

[layer_names]

3d_physics/layer_20="VRGui"

[rendering]

environment/default_environment="res://default_env.tres"
